{
  "compilerOptions": {
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["./src/*"],
      "@components": ["./src/components/index"],
      "@components/*": ["./src/components/*"],
      "@api": ["./src/api/index"],
      "@api/*": ["./src/api/*"],
      "@schemas": ["./src/schemas/index"],
      "@schemas/*": ["./src/schemas/*"],
      "@pages": ["./src/pages/index"],
      "@pages/*": ["./src/pages/*"],
      "@utils": ["./src/utils/index"],
      "@utils/*": ["./src/utils/*"]
    }
  },
  "include": ["src"]
}